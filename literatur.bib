@article{xieUsingAirborneVector2021,
  title = {Using Airborne Vector Magnetic Data to Calculate the Projection of the Magnetic Anomaly Vector onto a Normal Geomagnetic Field},
  author = {Xie, Rukuan and Xiong, Shengqing and Duan, Shuling and Wang, Jinlong and Wang, Ping and Luo, Yao and Liu, Haojun and Fan, Zhengguo},
