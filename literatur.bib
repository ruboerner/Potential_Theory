@article{smith1990,
  title = {Gridding with Continuous Curvature Splines in Tension},
  author = {Smith, W. H. F. and Wessel, P.},
  date = {1990-03},
  journaltitle = {GEOPHYSICS},
  shortjournal = {GEOPHYSICS},
  volume = {55},
