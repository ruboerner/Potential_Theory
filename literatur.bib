@article{kilburn2023,
  title = {Potential for Rupture before Eruption at {{Campi Flegrei}} Caldera, {{Southern Italy}}},
  author = {Kilburn, Christopher R. J. and Carlino, Stefano and Danesi, Stefania and Pino, Nicola Alessandro},
  date = {2023-06-09},
  journaltitle = {Communications Earth \& Environment},
  shortjournal = {Commun Earth Environ},
