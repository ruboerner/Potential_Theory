  title = {An Open-Source Full {{3D}} Electromagnetic Modeler for {{1D VTI}} Media in {{Python}}: Empymod},
  shorttitle = {An Open-Source Full {{3D}} Electromagnetic Modeler for {{1D VTI}} Media in {{Python}}},
  author = {Werthmüller, Dieter},
  date = {2017-11-01},
  journaltitle = {GEOPHYSICS},
