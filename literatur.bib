@article{pavon-carrasco2020,
  title = {Bootstrapping {{Swarm}} and Observatory Data to Generate Candidates for the {{DGRF}} and {{IGRF-13}}},
  author = {Pavón-Carrasco, F. Javier and Marsal, Santiago and Torta, J. Miquel and Catalán, Manuel and Martín-Hernández, Fátima and Tordesillas, J. Manuel},
